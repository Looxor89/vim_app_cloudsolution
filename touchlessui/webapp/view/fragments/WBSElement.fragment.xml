    	<Dialog id="wbselementDialog" title=" Search WBS Element help">
		<subHeader>
			<Toolbar design="Info">
				<ToolbarSpacer />
				<Label text="Recommendations" />
				<Switch state="true" change="onSwitchKOSTL">
					<layoutData>
						<FlexItemData growFactor="1" />
					</layoutData>
				</Switch>
			</Toolbar>
		</subHeader>

		<content>
			
			<List items="{path: 'appmodel>/valuehelps/WBSelement'}" 
				visible="true" mode="SingleSelectMaster" id="idWBSElement_VH" 
				selectionChange="onConfirmWBSElem">
				<CustomListItem>
					<HBox>
						<VBox class="sapUiSmallMarginBegin sapUiSmallMarginTopBottom">
							<Title text="{appmodel>value}"/>
							<ObjectStatus class="sapUiSmallMarginBottom" title="Confidence" text="{path:'appmodel>probability', formatter: '.formatter.formatConf'}" active="true" state="{path:'appmodel>probability', formatter: '.formatter.formatConfState'}" inverted="true" />
						</VBox>
					</HBox>
				</CustomListItem>
			</List>
			<List visible="{= !${appmodel>/props/ior_kostl}}" mode="SingleSelectMaster" items="{path:'APModel>/wbselement2Set'}"
				selectionChange="onConfirmWBSElem">
				<headerToolbar>
					<Toolbar>
						<SearchField search="onSearchWBSElem" width="100%" placeholder="WBS Elemement #" visible="true" />
					</Toolbar>
				</headerToolbar>
                <CustomListItem>
					<HBox>
						<VBox class="sapUiSmallMarginBegin sapUiSmallMarginTopBottom">
							<Title text="{APModel>Posid}"/>
							<!-- <ObjectStatus class="sapUiSmallMarginBottom" title="Confidence" text="{path:'appmodel>probability', formatter: '.formatter.formatConf'}" active="true" state="{path:'appmodel>probability', formatter: '.formatter.formatConfState'}" inverted="true" /> -->
						</VBox>
					</HBox>
				</CustomListItem>
			</List>
		</content>

		<endButton>
			<Button text="Cancel" press="onCancelWBSElem" />
		</endButton>
	</Dialog>
</core:FragmentDefinition>