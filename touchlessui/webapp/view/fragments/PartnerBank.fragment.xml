<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:core="sap.ui.core">
	<Dialog id="idPartnerBankDialog_VH" title="Partner Bank Search help">
		<subHeader>
			<Toolbar design="Info">
				<ToolbarSpacer />
				<Label text="Recommendations" />
				<Switch state="true" change="onSwitchKOSTL">
					<layoutData>
						<FlexItemData growFactor="1" />
					</layoutData>
				</Switch>
			</Toolbar>
		</subHeader>

		<content>
			
			<List items="{path: 'appmodel>/valuehelps/Partnerbanktype'}" 
				visible="true" mode="SingleSelectMaster" id="idPertnerBank_VH"
				selectionChange="onConfirmPartnerBank">
				<CustomListItem>
					<HBox>
						<VBox class="sapUiSmallMarginBegin sapUiSmallMarginTopBottom">
							<Title text="{appmodel>value}"/>
							<ObjectStatus class="sapUiSmallMarginBottom" title="Confidence" text="{path:'appmodel>probability', formatter: '.formatter.formatConf'}" active="true" state="{path:'appmodel>probability', formatter: '.formatter.formatConfState'}" inverted="true" />
						</VBox>
					</HBox>
				</CustomListItem>
			</List>
			<List items="{path: 'appmodel>/valuehelps/partbank'}"  mode="SingleSelectMaster" id="idPartnerBankVH"
				selectionChange="onBankSelect" visible="{ parts: [ 'appmodel>/props/POMode','appmodel>/detail/edited/Vendor'], formatter: '.formatter.isVendor' }">
				<!-- <headerToolbar>
					<Toolbar>
						<SearchField search="onSearchCostCenter2" width="100%" placeholder="PARTNER BANK #" visible="true" />
					</Toolbar>
				</headerToolbar> -->
				<StandardListItem
				title="{appmodel>Bvtyp}"
				
				/>
				
			</List>
			
		</content>

		<endButton>
			<Button text="Cancel" press="onCancelPartnerBankVH" />
		</endButton>
	</Dialog>
</core:FragmentDefinition>