{
  "welcomeFile": "/index.html",
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "^/sap/opu/odata/sap/API_COMPANYCODE_SRV/(.*)$",      
      "destination": "m19https",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "^/sap/opu/odata/sap/C_SUPPLIER_FS_SRV/(.*)$",      
      "destination": "m19https",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "^/sap/opu/odata/sap/C_GLACCOUNTVALUEHELP_CDS/(.*)$",      
      "destination": "m19https",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "^/sap/opu/odata/sap/API_COSTCENTER_SRV/(.*)$",      
      "destination": "m19https",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "^/sap/opu/odata/sap/API_PROFITCENTER_SRV/(.*)$",      
      "destination": "m19https",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "^/sap/opu/odata/sap/PS_WBSELEMENT_OVW_SRV/(.*)$",      
      "destination": "m19https",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "^/sap/opu/odata/sap/API_PURCHASEORDER_PROCESS_SRV/(.*)$",      
      "destination": "m19https",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "^/sap/opu/odata/sap/FAC_GL_JOURNALENTRY_VER_SRV/(.*)$",      
      "destination": "m19https",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "^/sap/opu/odata/sap/C_BILLGDOCREDACTED/(.*)$",      
      "destination": "m19https",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "^/sap/(.*)$",
      "target": "/sap/$1",
      "destination": "abap-cloud-abap-scb-cg-na-blockchain-dev",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "^/resources/(.*)$",
      "target": "/resources/$1",
      "authenticationType": "none",
      "destination": "ui5"
    },
    {
      "source": "^/test-resources/(.*)$",
      "target": "/test-resources/$1",
      "authenticationType": "none",
      "destination": "ui5"
    },
    {
      "source": "^(.*)$",
      "target": "$1",
      "service": "html5-apps-repo-rt",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^/sql/(.*)$",
      "target": "/v1/api/$1",
      "destination": "APOnPrem"
    },
    {
      "source": "^/api/dox/(.*)$",
      "target": "$1",
      "destination": "BTP_AISERVICE_DOX"
    },
    {
      "source": "^/api/objstore/(.*)$",
      "target": "/api/objstore/$1",
      "destination": "OBJSTORE_API_DEST"
    },
    {
      "source": "^/api/(.*)$",
      "target": "/v1/api/$1",
      "destination": "vim_app_cloud_backend-backend-api",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^/darbackend/(.*)$",
      "target": "/v1/api/$1",
      "destination": "darmta-backend-api"
    }
  ]
}
