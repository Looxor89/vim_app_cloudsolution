<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		id="idAssignToDialog"
		noDataText="No Users found"
		title="{detailDetailModel>/sDialogTitle}"
		search=".onAssignSearch"
		confirm=".onAssignConfirm"
		cancel=".onAssignClose"
		items="{
			path: 'UserList>/Users'
		}">
		<StandardListItem
			title="{UserList>FirstName} {UserList>LastName}"
			description="{UserList>Email}"
			icon="sap-icon://employee"
			iconDensityAware="false"
			iconInset="false"
			type="Active" />
	</SelectDialog>
</core:FragmentDefinition>