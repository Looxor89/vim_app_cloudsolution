<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:core="sap.ui.core">
	<SelectDialog 
		id="idHouseBankDialog_VH" 
		noDataText="{i18n>NoHouseBanksFound}"
		items="{path: 'detailDetailModel>/valuehelps/houseBanks'}"
		title="{i18n>TitleSearchHouseBanks}"
		search="onSearchHouseBanks"
		confirm="onConfirmHouseBanks">
		<StandardListItem 
			title="{detailDetailModel>HouseBank}" 
			description="{detailDetailModel>BankName} | {detailDetailModel>SWIFTCode} | {detailDetailModel>Bank}"/>	
	</SelectDialog>
</core:FragmentDefinition>