<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:core="sap.ui.core">
	<TableSelectDialog 
		id="idServiceEntrySheetItemRefDialog_VH" 
        resizable="true"
		noDataText="{i18n>NoServiceEntrySheetsFound}"
		items="{path: 'detailDetailModel>/valuehelps/serviceEntrySheetItemReferements'}"
		title="{i18n>TitleSearchServiceEntrySheetsItem}"
		search="onSearchServiceEntrySheetsItem"
		confirm="onConfirmServiceEntrySheetItem">
        <ColumnListItem vAlign="Middle">
            <cells>
                <ObjectIdentifier
                    title="{detailDetailModel>ServiceEntrySheet}"/>
                <Text
                    text="{detailDetailModel>ServiceEntrySheetItem}" />
                <Text
                    text="{detailDetailModel>AccountAssignmentCategory}" />
                <Text
                    text="{detailDetailModel>CreatedByUser}" />
                <Text
                    text="{detailDetailModel>CreationDateTime}" />
                <Text
                    text="{detailDetailModel>PurchasingOrganization}" />
                <Text
                    text="{detailDetailModel>PurchasingGroup}" />
                <Text
                    text="{detailDetailModel>Plant}" />
                <Text
                    text="{detailDetailModel>Currency}" />
                <Text
                    text="{detailDetailModel>MaterialGroup}" />
                <Text
                    text="{detailDetailModel>PurchaseOrder}" />
                <Text
                    text="{detailDetailModel>PurchaseOrderItem}" />
            </cells>
        </ColumnListItem>
		<columns>
			<Column width="12em">
				<header>
					<Text text="{i18n>To_SelectedServiceEntrySheets_ServiceEntrySheet}" />
				</header>
			</Column>
			<Column
				minScreenWidth="Tablet"
				demandPopin="true"
				hAlign="End">
				<header>
					<Text text="{i18n>To_SelectedServiceEntrySheets_ServiceEntrySheetItem}" />
				</header>
			</Column>
			<Column
				minScreenWidth="Desktop"
				demandPopin="true"
				hAlign="Center">
				<header>
					<Text text="{i18n>AccountAssignmentCategory}" />
				</header>
			</Column>
			<Column hAlign="End">
				<header>
					<Text text="{i18n>CreatedByUser}" />
				</header>
			</Column>
			<Column hAlign="End">
				<header>
					<Text text="{i18n>CreationDateTime}" />
				</header>
			</Column>
			<Column hAlign="End">
				<header>
					<Text text="{i18n>PurchasingOrganization}" />
				</header>
			</Column>
			<Column hAlign="End">
				<header>
					<Text text="{i18n>PurchasingGroup}" />
				</header>
			</Column>
			<Column hAlign="End">
				<header>
					<Text text="{i18n>Plant}" />
				</header>
			</Column>
			<Column hAlign="End">
				<header>
					<Text text="{i18n>Currency}" />
				</header>
			</Column>
			<Column hAlign="End">
				<header>
					<Text text="{i18n>MaterialGroup}" />
				</header>
			</Column>
			<Column hAlign="End">
				<header>
					<Text text="{i18n>PurchaseOrder}" />
				</header>
			</Column>
			<Column hAlign="End">
				<header>
					<Text text="{i18n>PurchaseOrderItem}" />
				</header>
			</Column>
		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>