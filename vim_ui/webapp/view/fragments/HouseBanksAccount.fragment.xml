<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:core="sap.ui.core">
	<SelectDialog 
		id="idHouseBanksAccountDialog_VH" 
		noDataText="{i18n>NoHouseBanksAccountFound}"
		items="{path: 'detailDetailModel>/valuehelps/houseBanksAccount'}"
		title="{i18n>TitleSearchHouseBanksAccount}"
		search="onSearchHouseBanksAccount"
		confirm="onConfirmHouseBanksAccount">
		<StandardListItem 
			title="{detailDetailModel>HouseBankAccount}" 
			description="{detailDetailModel>BankAccountDescription} | {detailDetailModel>IBAN}"/>	
	</SelectDialog>
</core:FragmentDefinition>